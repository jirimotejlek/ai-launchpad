# PostgreSQL Database Configuration
# Copy this file to .env.postgres in the project root and customize the values:
# cp env_templates/postgres.env .env.postgres

# Database Name
POSTGRES_DB=ai_launchpad

# Database User (PostgreSQL superuser)
POSTGRES_USER=admin

# Database Password (REQUIRED - Change this to a secure password!)
# Generate a strong password: At least 16 characters, mixed case, numbers, and symbols
# Example generators:
#   - openssl rand -base64 24
#   - pwgen -s 24 1
POSTGRES_PASSWORD=your_secure_password_here

